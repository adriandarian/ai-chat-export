{
  "name": "ai-chat-export",
  "version": "1.0.0",
  "description": "Browser Extension to export any chat",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env BROWSER=chrome vite",
    "dev:chrome": "cross-env BROWSER=chrome vite",
    "dev:firefox": "cross-env BROWSER=firefox vite",
    "dev:safari": "cross-env BROWSER=safari vite",
    "build:base": "tsc && vite build",
    "build": "npm run build:chrome",
    "build:chrome": "cross-env BROWSER=chrome npm run build:base",
    "build:firefox": "cross-env BROWSER=firefox npm run build:base",
    "build:safari": "cross-env BROWSER=safari npm run build:base",
    "build:edge": "cross-env BROWSER=edge npm run build:base",
    "build:opera": "cross-env BROWSER=opera npm run build:base",
    "build:brave": "cross-env BROWSER=brave npm run build:base",
    "build:all": "npm run build:chrome && npm run build:firefox && npm run build:safari && npm run build:edge && npm run build:opera && npm run build:brave",
    "build:chromium-all": "npm run build:chrome && npm run build:edge && npm run build:opera && npm run build:brave",
    "build:analyze": "cross-env ANALYZE=true npm run build:chrome",
    "build:analyze:all": "cross-env ANALYZE=true npm run build:all",
    "bundle:check": "bun run scripts/analyze-bundle.ts chrome --ci",
    "bundle:check:all": "bun run scripts/analyze-bundle.ts chrome --ci && bun run scripts/analyze-bundle.ts firefox --ci && bun run scripts/analyze-bundle.ts safari --ci",
    "bundle:report": "bun run scripts/analyze-bundle.ts chrome --json",
    "package": "npm run build:all && npm run zip:all",
    "zip:chrome": "cd dist/chrome && zip -r ../../packages/ai-chat-export-chrome.zip . -x '*.map'",
    "zip:firefox": "cd dist/firefox && zip -r ../../packages/ai-chat-export-firefox.zip . -x '*.map'",
    "zip:safari": "cd dist/safari && zip -r ../../packages/ai-chat-export-safari.zip . -x '*.map'",
    "zip:edge": "cd dist/edge && zip -r ../../packages/ai-chat-export-edge.zip . -x '*.map'",
    "zip:opera": "cd dist/opera && zip -r ../../packages/ai-chat-export-opera.zip . -x '*.map'",
    "zip:brave": "cd dist/brave && zip -r ../../packages/ai-chat-export-brave.zip . -x '*.map'",
    "zip:all": "mkdir -p packages && npm run zip:chrome && npm run zip:firefox && npm run zip:safari && npm run zip:edge && npm run zip:opera && npm run zip:brave",
    "clean": "rm -rf dist packages bundle-reports",
    "preview": "vite preview",
    "semantic-release": "semantic-release",
    "semantic-release:dry-run": "semantic-release --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adriandarian/ai-chat-export.git"
  },
  "keywords": [
    "browser-extension",
    "chrome-extension",
    "firefox-extension",
    "safari-extension",
    "chat-export",
    "ai-chat"
  ],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "clsx": "^2.1.0",
    "lucide-react": "^0.555.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.0.0-beta.28",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/archiver": "^7.0.0",
    "@types/chrome": "^0.1.31",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "archiver": "^7.0.0",
    "autoprefixer": "^10.4.22",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "cross-env": "^10.1.0",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.5",
    "semantic-release": "^25.0.2",
    "tailwindcss": "4.1.17",
    "typescript": "^5.3.3",
    "vite": "^7.2.4"
  },
  "bugs": {
    "url": "https://github.com/adriandarian/ai-chat-export/issues"
  },
  "homepage": "https://github.com/adriandarian/ai-chat-export#readme"
}
